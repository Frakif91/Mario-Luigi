config_version=5

[application]

config/name="Mario & Luigi : Hotel Mania"
config/version="1.2.5"
run/main_scene="res://Godot/Scene/title.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://Assets/load.png"
config/icon="res://n_icon.png"

[autoload]

Globals="*res://Godot/Scripts/Utility/globals.gd"
Transitions="*res://Godot/Nodes/transitions.tscn"
Debugger="*res://Godot/Scripts/Utility/Debugging.gd"
GlobalSettings="*res://Godot/Scripts/Utility/ChangeGlobalFiles.gd"
MultiplayerInfo="*res://Godot/Scripts/Multiplayer/multiplayer_info.gd"

[debug]

multirun/number_of_windows=2
multirun/add_custom_args=false
multirun/window_args=["listen", "join"]

[display]

window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Mario & Luigi"

[editor]

movie_writer/movie_file="D:/Mario & Luigi/Mario-Luigi/Movie.avi"
movie_writer/fps=30

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Assets/": "purple",
"res://Assets/3D/": "red",
